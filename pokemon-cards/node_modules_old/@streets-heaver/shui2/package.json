  "scripts": {
    "start": "storybook dev -p 6006 ./IconImages",
    "storybook": "storybook dev -p 6006 ./IconImages",
    "build-storybook": "storybook build ./IconImages --quiet",
    "build-lib": "tsc  && rollup -c --esModule",
    "publish-local": "npm run build-lib && copy package.json dist && copy .npmrc dist && xcopy /s /i /y dist %USERPROFILE%\\source\\repos\\shui2\\dist",
    "rmdir": "node -e \"var fs = require('fs'); process.argv.slice(1).map((fpath) => fs.rmdirSync(fpath, { recursive: true })); process.exit(0);\"",
    "build-publish": "npm run build-lib && copy package.json dist && copy .npmrc dist && cd dist && npm publish",
    "build-publish-cleanup": "(npm run build-publish && npm run rmdir -- dist) || npm run rmdir -- dist",
    "test-storybook": "test-storybook --ci --junit --no-cache",
    "build-for-tests": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run build-storybook && npx http-server storybook-static --port 6006 --silent\" \"wait-on http://127.0.0.1:6006 && npm run test-storybook\"",
    "lint": "npx eslint **/src/**/*.js **/src/**/*.jsx"
